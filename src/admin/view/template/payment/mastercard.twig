            <label class="section-description" for="integeration-settings" class="col-12">{{ intgrn_sec_desc }}</label>
            <div class="row settings-division">
              <label for="module-status" class="col-sm-2 col-form-label">{{ entry_status }}</label>
              <div class="col-sm-10">
                <div class="form-check form-switch form-switch-lg">
                  <input type="checkbox" name="payment_mastercard_status" value="1" id="payment_mastercard_status"
                    class="form-check-input" {{ payment_mastercard_status ? 'checked' : '' }}>
                    <label class="status-label"> Enabled </label>
                    
                </div>
                <label class="mastercard-admin-notes"> {{ info_hc }}</label>
              </div>
            </div>

            <div class="row settings-division required">
              <label for="entry-title" class="col-sm-2 col-form-label">{{ entry_title }}</label>
              <div class="col-sm-10">
                {% if payment_mastercard_title is empty %}
                <input type="text" name="payment_mastercard_title" value="{{ payment_mastercard_title }}"
                  id="entry-title" class="form-control is-invalid" />
                <div class="invalid-feedback">
                  Please enter the Title for this payment method.
                </div>
                {% else %}
                <input type="text" name="payment_mastercard_title" value="{{ payment_mastercard_title }}"
                  id="entry-title" class="form-control" />
                {% endif %}
                <label class="mastercard-admin-notes"> {{ info_title }}</label>
              </div>
            </div>


            <div class="row settings-division">
              <label for="initial-transaction" class="col-sm-2 col-form-label">{{ entry_initial_transaction }}</label>
