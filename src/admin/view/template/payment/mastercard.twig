{{ header }}{{ column_left }}
<div id="content">
  <div class="page-header">
    <div class="container-fluid">
      {% if error_warning %}
      <div class="alert alert-danger alert-dismissible"><i class="fa-solid fa-circle-exclamation"></i> {{ error_warning
        }} <button type="button" class="btn-close" data-bs-dismiss="alert"></button></div>
      {% endif %}
      {% if error_credentials_validation %}
      <div class="alert alert-danger alert-dismissible"><i class="fa-solid fa-circle-exclamation"></i> {{
        error_credentials_validation }} <button type="button" class="btn-close" data-bs-dismiss="alert"></button></div>
      {% endif %}
      <div class="float-end">
        <button type="submit" form="form-mastercard" data-bs-toggle="tooltip" title="{{ button_save }}"
          class="btn btn-primary"><i class="fa-solid fa-floppy-disk"></i></button>
        <a href="{{ back }}" data-bs-toggle="tooltip" title="{{ button_back }}" class="btn btn-light"><i
            class="fa-solid fa-reply"></i></a>
      </div>
      <h1>{{ heading_title }}</h1>
      <ol class="breadcrumb">
        {% for breadcrumb in breadcrumbs %}
        <li class="breadcrumb-item"><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
        {% endfor %}
      </ol>
      <div class="version-info">
        <div class="module-version"><strong>{{ entry_module_version }}</strong> <span>{{ module_version }}</span></div>
        <div class="api-version"><strong>{{ entry_api_version }}</strong> <span>{{ api_version }}</span></div>

        {% if latest_version > module_version %}
        <div class="alert alert-warning alert-dismissible module-notification-wrapper">
          <i class="fa-solid fa-circle-exclamation"></i> Warning: {{ update_message }}
          <button type="button" class="btn-close" data-bs-dismiss="alert"></button>
        </div>
        {% endif %}
      </div>

    </div>
  </div>

  <div class="container-fluid">
    <div class="card">
      <div class="card-header"><i class="fa-solid fa-pencil"></i> {{ text_edit }}</div>
      <div class="card-body">
        <form id="form-mastercard" action="{{ action }}" method="post" enctype="multipart/form-data">
